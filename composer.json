{
    "name": "serlo-org/athene2-versioning",
    "description": "Zend Framework 2 Module that provides versioning components for Athene2",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "module",
        "zf2",
        "athene2",
        "core",
        "common"
    ],
    "homepage": "https://github.com/serlo-org/athene2-versioning",
    "authors": [
        {
            "name": "Aeneas Rekkas"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zend-mvc": "~2.3",
        "zendframework/zend-servicemanager": "~2.3",
        "zendframework/zend-filter": "~2.3",
        "zendframework/zend-form": "~2.3",
        "zendframework/zend-stdlib": "~2.3",
        "zendframework/zend-eventmanager": "~2.3",
        "zendframework/zend-paginator": "~2.3",
        "zendframework/zend-authentication": "~2.3",
        "zendframework/zend-validator": "~2.3",
        "doctrine/common": "~2.4",
        "doctrine/doctrine-module": "~0.8",
        "doctrine/doctrine-orm-module": "~0.8",
        "serlo-org/athene2-common": "1.0",
        "zf-commons/zfc-rbac": "2.3"
    },
    "require-dev": {
        "zendframework/zendframework": "~2.2",
        "phpunit/phpunit": "~3.7",
        "squizlabs/php_codesniffer": "1.4.*",
        "satooshi/php-coveralls": "~0.6"
    },
    "autoload": {
        "psr-0": {
            "Versioning\\": "src/"
        }
    }
}